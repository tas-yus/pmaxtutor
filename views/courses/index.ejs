<% include ../partials/header %>
<div class="container">
    <div class="row">
    <% courses.forEach((course) => { %>
        <div class="col-md-4 col-sm-6">
            <div class="thumbnail"><img src="/assets/images/<%= course.image%>"></div>
            <div class="caption">
                <h3><%= course.title %></h3>
                <p><%= course.description %></p>
            </div>
            <a href="/<%= course.code %>" class="btn btn-primary">Read More</a>
        </div>
    <% }); %>
    </div>
</div>
<% include ../partials/footer %>
